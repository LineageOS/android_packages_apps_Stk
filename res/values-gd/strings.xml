<resources>
    <!-- Default Application label used on home screen with an icon -->
    <string name="app_name">Acainnean SIM</string>
    <!-- Menu button option for ending current SIM session -->
    <string name="menu_end_session">Cuir crìoch air an t-seisean</string>
    <!-- Menu button option for requesting additional help information -->
    <string name="help">Cobhair</string>
    <!-- Menu button option to navigate back to the previous screen -->
    <string name="menu_back">Air ais</string>
    <!-- Settings entry which describes the current SIM Toolkit service on the device -->
    <string name="service_name">Ainm na seirbhise</string>
    <!-- Default description for service_name when no STK service is available -->
    <string name="stk_no_service">Chan eil seirbheis ri làimh</string>
    <!-- OK button used to confirm a message dialog -->
    <string name="button_ok">Ceart ma-thà</string>
    <!-- Cancel button used to dismiss a message dialog and terminate the current SIM session-->
    <string name="button_cancel">Sguir dheth</string>
    <!-- Yes button, used on a Yes/No confirmation screen -->
    <string name="button_yes">Tha</string>
    <!-- No button, used on a Yes/No confirmation screen -->
    <string name="button_no">Chan eil</string>
    <!-- Description for the type of text allowed in a form: alphanumeric or digits only -->
    <string name="alphabet">Aibidilean</string>
    <!-- Type of alphabet text allowed in a form: 0-9, *, #, + characters only -->
    <string name="digits">Àireamhan (0-9, *, #, +)</string>
    <!-- Default transition message when launching a call -->
    <string name="default_call_setup_msg">Gairm a’ dol air adhart\u2026</string>
    <!-- Default message for SET_UP_CALL when there is no ALPHA tag in it -->
    <string name="default_setup_call_msg">Gairm ’ga ullachadh</string>
    <string name="stk_app_state">Staid na h-aplacaid</string>
    <string name="enable_app">An comas</string>
    <string name="disable_app">À comas</string>
    <!-- Title text used for system dialog from STK app -->
    <string name="stk_dialog_title">Acainnean SIM</string>
    <!-- Default message when no alpha tag in Play Tone -->
    <string name="default_tone_dialog_msg">Seirm cluiche</string>
    <!-- Default message when no alpha in OPEN_CHANNEL -->
    <string name="default_open_channel_msg">A bheil thu airson an t-seanail fhosgladh?</string>
    <!-- Default message when no alpha in SEND_DATA -->
    <string name="default_send_data_msg">A’ cur dàta</string>
    <!-- Default message when no alpha in RECEIVE_DATA -->
    <string name="default_receive_data_msg">A’ faighinn dàta</string>
    <!-- Default message when no alpha in CLOSE_CHANNEL-->
    <string name="default_close_channel_msg">Seanail dhùinte</string>
    <!-- Button texts used in STK dialog -->
    <string name="stk_dialog_accept">THA</string>
    <string name="stk_dialog_reject">CHAN EIL</string>
    <string name="no_sim_card_inserted">Cuir a-steach SIM airson na h-acainnean SIM a chur gu dol.</string>
    <string name="stk_channel_name">Teachdaireachdan seirbheis mobile</string>
</resources>
